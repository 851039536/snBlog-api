{

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": ".\\LogFiles\\log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;database=snblog;uid=root;pwd=woshishui;"
  },
  "AllowedHosts": "*",
  "Authentication": { //jwt
    "JwtBearer": {
      "IsEnabled": "true",
      "SecurityKey": "Demo_C421AAEE0D114E9C1", //签名秘钥     
      "Issuer": "少年", //颁发者    
      "Audience": "少年", //接收者  
      "Expiration": 120//token过期时间 （单位：分钟）
    }

  }
}
